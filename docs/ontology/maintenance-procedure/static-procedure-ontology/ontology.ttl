@prefix : <http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#> .

<http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology> rdf:type owl:Ontology ;
                                                                                    owl:versionIRI <http://spec.equonto.com/ontology/202312/maintenance-procedure/static-procedure-ontology> ;
                                                                                    owl:imports <http://rds.posccaesar.org/ontology/lis14/ont/core> ,
                                                                                                <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/> ;
                                                                                    <http://purl.org/dc/terms/abstract> "The OMPD Static Procedure Ontology captures static information found in most insdutrial maintenance procedures." ;
                                                                                    <http://purl.org/dc/terms/contributor> "Caitlin Woods: The University of Western Australia and Equonto" ,
                                                                                                                           "Melinda Hodkiewicz: The University of Western Australia" ,
                                                                                                                           "Tim French: The University of Western Australia" ;
                                                                                    <http://purl.org/dc/terms/creator> "The University of Western Australia & Equonto Pty. Ltd." ;
                                                                                    <http://purl.org/dc/terms/license> "http://opensource.org/licenses/MIT" ;
                                                                                    <http://purl.org/dc/terms/publisher> "Equonto Pty. Ltd." ;
                                                                                    <http://purl.org/dc/terms/title> "Ontology for Maintenance Procedure Documentation (OMPD) Static Procedure Ontology" ;
                                                                                    rdfs:label "Maintenance Procedures - Static Procedure Ontology" ;
                                                                                    <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .

#################################################################
#    Object Properties
#################################################################

###  http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart
<http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/hasQuality
<http://rds.posccaesar.org/ontology/lis14/rdl/hasQuality> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/hasRole
<http://rds.posccaesar.org/ontology/lis14/rdl/hasRole> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/isAbout
<http://rds.posccaesar.org/ontology/lis14/rdl/isAbout> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo
<http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn
<http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/representedBy
<http://rds.posccaesar.org/ontology/lis14/rdl/representedBy> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/roleOf
<http://rds.posccaesar.org/ontology/lis14/rdl/roleOf> rdf:type owl:ObjectProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#directActivityPartOf
:directActivityPartOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :indirectActivityPartOf ;
                      owl:inverseOf :hasDirectActivityPart ;
                      rdf:type owl:IrreflexiveProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#directlyActivityPartOf
:directlyActivityPartOf rdf:type owl:ObjectProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#directlyAfter
:directlyAfter rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :indirectlyAfter .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#directlyBefore
:directlyBefore rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :indirectlyBefore ;
                owl:inverseOf :directlyAfter ;
                rdf:type owl:FunctionalProperty ,
                         owl:IrreflexiveProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#hasDirectActivityPart
:hasDirectActivityPart rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasIndirectActivityPart .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#hasIndirectActivityPart
:hasIndirectActivityPart rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart> ;
                         rdf:type owl:TransitiveProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#hasRequirement
:hasRequirement rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/hasParticipant> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#indirectActivityPartOf
:indirectActivityPartOf rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/activityPartOf> ;
                        owl:inverseOf :hasIndirectActivityPart ;
                        rdf:type owl:TransitiveProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#indirectlyAfter
:indirectlyAfter rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/after> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#indirectlyBefore
:indirectlyBefore rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/before> ;
                  owl:inverseOf :indirectlyAfter ;
                  rdf:type owl:TransitiveProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#locallyEquivalentTo
:locallyEquivalentTo rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :reasoningSupport ;
                     rdf:type owl:SymmetricProperty ;
                     rdfs:domain :MaintenanceTask ;
                     rdfs:range :MaintenanceTask .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#reasoningSupport
:reasoningSupport rdf:type owl:ObjectProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#representedBy
:representedBy rdf:type owl:ObjectProperty .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#requirementOf
:requirementOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/participantIn> ;
               owl:inverseOf :hasRequirement .


#################################################################
#    Data properties
#################################################################

###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#documentIdentifier
:documentIdentifier rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:label "documentIdentifier" .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#hasImageUrl
:hasImageUrl rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:label "hasImageUrl" .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#hasTextValue
:hasTextValue rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:label "hasTextValue" .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#hasVersion
:hasVersion rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:label "hasVersion" .


#################################################################
#    Classes
#################################################################

###  http://rds.posccaesar.org/ontology/lis14/rdl/Activity
<http://rds.posccaesar.org/ontology/lis14/rdl/Activity> rdf:type owl:Class .


###  http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity
<http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity> rdf:type owl:Class .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#Component
:Component rdf:type owl:Class ;
           rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ;
           rdfs:comment "A ISO:PhysicalObject that  may have some OMPD:ResourceRole" ;
           rdfs:label "Component" ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#Hazard
:Hazard rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity>
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn> ;
                                                     owl:someValuesFrom :HazardRealizationProcess
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Disposition> ;
        rdfs:comment "An ISO: Realizeable Entity that is realizrd in some OMPD: Hazard Realization Process" ;
        rdfs:label "Hazard" ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/logicAxiom> "Hazard _ RealizeableEntity _ (_realizedIn.HazardRealizationProcess)" ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#HazardInMaintenanceProcess
:HazardInMaintenanceProcess rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( :Hazard
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn> ;
                                                                         owl:someValuesFrom [ owl:intersectionOf ( :HazardRealizationProcess
                                                                                                                   [ rdf:type owl:Restriction ;
                                                                                                                     owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo> ;
                                                                                                                     owl:someValuesFrom :MaintenanceProcess
                                                                                                                   ]
                                                                                                                 ) ;
                                                                                              rdf:type owl:Class
                                                                                            ]
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Disposition> ;
                            rdfs:comment "An OMPD: Hazard that is realized in some OMPD: Hazard Realization Process that occurs relative to some OMPD: Maintenance Process" ;
                            rdfs:label "HazardInMaintenanceProcess" ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#HazardRealizationProcess
:HazardRealizationProcess rdf:type owl:Class ;
                          rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Activity> ;
                          rdfs:comment "An ISO: Activity that has the potential to cause harm" ;
                          rdfs:label "HazardRealizationProcess" ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItem
:MaintainableItem rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject>
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/hasRole> ;
                                                               owl:someValuesFrom :MaintainableItemRole
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ;
                  rdfs:comment "An ISO:PhysicalObject hat has a role OMPD:MaintainableItemRole" ;
                  rdfs:label "MaintainableItem" ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItemRole
:MaintainableItemRole rdf:type owl:Class ;
                      rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Role> ;
                      rdfs:comment "An ISO:Role that is the role of the item that work is performed on in a OMPD:MaintenanceProcess" ;
                      rdfs:label "MaintainableItemRole" ;
                      <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
                      <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureDocument
:MaintenanceProcedureDocument rdf:type owl:Class ;
                              owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject>
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/isAbout> ;
                                                                           owl:someValuesFrom :MaintenanceProcedureProcess
                                                                         ]
                                                                       ) ;
                                                    rdf:type owl:Class
                                                  ] ;
                              rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> ;
                              rdfs:comment "An ISO:InformationObject that is about some OMPD:MaineanceProcedureProcess" ;
                              rdfs:label "MaintenanceProcedureDocument" ;
                              <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                              <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcedureProcess
:MaintenanceProcedureProcess rdf:type owl:Class ;
                             owl:equivalentClass [ owl:intersectionOf ( :MaintenanceProcess
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart> ;
                                                                          owl:someValuesFrom :MaintenanceTask
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/representedBy> ;
                                                                          owl:someValuesFrom :MaintenanceProcedureDocument
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ] ;
                             rdfs:subClassOf :MaintenanceProcess ;
                             rdfs:comment "An OMPD:MaintenanceProcess that is represented in some OMPD:MaintenanceProcedureDocument and has activity part some OMPD:MaintenanceTask" ;
                             rdfs:label "MaintenanceProcedureProcess" ;
                             <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                             <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceProcess
:MaintenanceProcess rdf:type owl:Class ;
                    rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Activity> ;
                    rdfs:comment "An ISO: Activity, intended to repair or restore an item to a state in which it can perform its required function" ;
                    rdfs:label "MaintenanceProcess" ;
                    <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
                    <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTask
:MaintenanceTask rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :MaintenanceProcess
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :representedBy ;
                                                              owl:someValuesFrom :MaintenanceTaskDescription
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :locallyEquivalentTo ;
                                                              owl:allValuesFrom :MaintenanceTask
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :directActivityPartOf ;
                                                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass :MaintenanceTask
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :directlyActivityPartOf ;
                                                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass :MaintenanceProcedureProcess
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :directlyBefore ;
                                                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                              owl:onClass :MaintenanceTask
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :MaintenanceProcess ;
                 rdfs:comment "An OMPD:MaintenanceProcess that is: - a direct activity part of zero or one OMPD:MaintenanceProcedureProcess - an activity part of zero or more OMPD:MaintenanceProcedureProcess - a direct activity part of zero or one OMPD:MaintenanceTask - an activity part of zero or more OMPD:MaintenanceTask - occurs directly before zero or one Maintenance task - occurs before zero or more MaintenanceTask - occurs directly after zero or one MaintenanceTask - occurs after zero or more OMPD:MaintenanceTask" ;
                 rdfs:label "MaintenanceTask" ;
                 <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                 <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#MaintenanceTaskDescription
:MaintenanceTaskDescription rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject>
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/isAbout> ;
                                                                         owl:someValuesFrom :MaintenanceTask
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> ;
                            rdfs:comment "An ISO:InformationObject that is about some OMPD:MaintenanceTask" ;
                            rdfs:label "MaintenanceTaskDescription" ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#Material
:Material rdf:type owl:Class ;
          rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ;
          rdfs:comment "A ISO:PhysicalObject that may have some OMPD:ResourceRole" ;
          rdfs:label "Material" ;
          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#Permit
:Permit rdf:type owl:Class ;
        rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> ;
        rdfs:comment "A ISO:InformationObject that  may have some OMPD:ResourceRole" ;
        rdfs:label "Permit" ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#Qualification
:Qualification rdf:type owl:Class ;
               rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Quality> ;
               rdfs:comment "A ISO:Quality that is beared by zero or more OMPD:QualitifiedPerson" ;
               rdfs:label "Qualification" ;
               <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
               <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#QualifiedPerson
:QualifiedPerson rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/Person>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/hasQuality> ;
                                                              owl:someValuesFrom :Qualification
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :requirementOf ;
                                                              owl:someValuesFrom <http://rds.posccaesar.org/ontology/lis14/rdl/Activity>
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Person> ;
                 rdfs:comment "A ISO:Person that may have some OMPD:ResourceRole and has a OMPD:Qualification" ;
                 rdfs:label "QualifiedPerson" ;
                 <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                 <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#Resource
:Resource rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/Object>
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/hasRole> ;
                                                       owl:someValuesFrom :ResourceRole
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Object> ;
          rdfs:comment "An ISO: Object that has some OMPD: Resource Role" ;
          rdfs:label "Resource" ;
          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .


###  http://spec.equonto.com/ontology/maintenance-procedure/static-procedure-ontology#ResourceRole
:ResourceRole rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/Role>
