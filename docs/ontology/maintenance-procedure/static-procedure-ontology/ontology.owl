    <!-- http://spec.equonto.com/ontology/maintenance-procedure#Component -->


    <owl:Class rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#Component">
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject"/>
        <rdfs:comment>A ISO:PhysicalObject that  may have some OMPD:ResourceRole</rdfs:comment>
        <rdfs:label>Component</rdfs:label>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
    </owl:Class>
    


    <!-- http://spec.equonto.com/ontology/maintenance-procedure#Hazard -->


    <owl:Class rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#Hazard">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.com/ontology/maintenance-procedure#HazardRealizationProcess"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Disposition"/>
        <rdfs:comment>An ISO: Realizeable Entity that is realizrd in some OMPD: Hazard Realization Process</rdfs:comment>
        <rdfs:label>Hazard</rdfs:label>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:logicAxiom>Hazard _ RealizeableEntity _ (_realizedIn.HazardRealizationProcess)</AnnotationVocabulary:logicAxiom>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
    </owl:Class>
    


    <!-- http://spec.equonto.com/ontology/maintenance-procedure#HazardInMaintenanceProcess -->


    <owl:Class rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#HazardInMaintenanceProcess">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#Hazard"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#HazardRealizationProcess"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo"/>
                                        <owl:someValuesFrom rdf:resource="http://spec.equonto.com/ontology/maintenance-procedure#MaintenanceProcess"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Disposition"/>
        <rdfs:comment>An OMPD: Hazard that is realized in some OMPD: Hazard Realization Process that occurs relative to some OMPD: Maintenance Process</rdfs:comment>
        <rdfs:label>HazardInMaintenanceProcess</rdfs:label>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
    </owl:Class>
    


