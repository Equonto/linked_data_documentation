        <rdfs:subClassOf rdf:resource="http://rds.posccaesar.org/ontology/lis14/rdl/Activity"/>
        <rdfs:comment>An ISO: Activity that has the potential to cause harm</rdfs:comment>
        <rdfs:label>HazardRealizationProcess</rdfs:label>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
    </owl:Class>
    


    <!-- http://spec.equonto.com/ontology/maintenance-procedure#MaintainableItem -->


    <owl:Class rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#MaintainableItem">
