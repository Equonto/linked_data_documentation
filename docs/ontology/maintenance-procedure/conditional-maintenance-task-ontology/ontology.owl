    <!-- http://spec.equonto.com/ontology/maintenance-procedure#CorrectiveMaintenanceTask -->


    <owl:Class rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#CorrectiveMaintenanceTask">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spec.equonto.com/ontology/maintenance-procedure#MaintenanceTask"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://spec.equonto.com/ontology/maintenance-procedure#addresses"/>
                        <owl:someValuesFrom rdf:resource="http://spec.equonto.com/ontology/maintenance-procedure#FunctionalFailure"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://spec.equonto.com/ontology/maintenance-procedure#MaintenanceTask"/>
        <rdfs:comment>A OMPD:maintenance task that addresses a OMPD:functional failure</rdfs:comment>
        <rdfs:label>Corrective Maintenance Task</rdfs:label>
        <AnnotationVocabulary:isPrimitive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</AnnotationVocabulary:isPrimitive>
        <AnnotationVocabulary:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released"/>
    </owl:Class>
    


